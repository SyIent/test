-- OrionLibrary
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source'))()

-- Window
local Window = OrionLib:MakeWindow({Name = "BLeed | HOTEL ELEPHANT", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- TeleportGameTab
